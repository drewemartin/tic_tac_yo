<tr class = "message-tr">
  <td><%=private_message.created_at.in_time_zone("Eastern Time (US & Canada)").to_formatted_s(:long) %></td>
  <td><%=private_message.sender.first_name %> <%=private_message.sender.last_name %></td>
  <td><%=private_message.message %></td>
  <td>  <%= link_to "View", private_message_path(private_message), class: "btn btn-success"%>
		<%= link_to "Delete", private_message_path(private_message), method: :delete,class: 'btn btn-danger delete-btn ajax-delete-private-message', remote: true %></td>
</tr>